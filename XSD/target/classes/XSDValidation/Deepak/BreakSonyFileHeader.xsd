<xs:schema targetNamespace="http://XSDVali/BreakSony/BreakSonyHeader" 
xmlns:xs="http://www.w3.org/2001/XMLSchema" 
xmlns="http://XSDVali/BreakSony/BreakSonyHeader" 
xmlns:jrb="http://jrecordbind.org/3/xsd" elementFormDefault="qualified">


	<xs:complexType name="FileHeader">
		<xs:sequence>
			<xs:element name="RecordType" type="xs:int" fixed="1" jrb:length="1" />
			<xs:element name="CreationDate" type="xs:int" jrb:length="8" />
			
			<xs:element name="CreationTime" type="xs:int" jrb:length="6" jrb:padder="org.fissore.jrecordbind.padders.ZeroLeftPadder" />
		</xs:sequence>

	</xs:complexType>
	<xs:element name="main" type="FileHeader" jrb:length="15" />

</xs:schema>
